{
  "manifest_version": 3,
  "name": "Itch.io Jam Calendar Buttons",
  "version": "1.0.0",
  "description": "Add calendar buttons to itch.io game jams.",
  "permissions": [],
  "host_permissions": [
    "https://itch.io/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://itch.io/jam/*"],
      "js": ["index.js"],
      "run_at": "document_idle"
    }
  ]
}
